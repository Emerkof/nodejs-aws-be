{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Create Product Schema",
  "required": ["title"],
  "properties": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "imageUrl": {
      "type": "string",
      "format": "uri"
    },
    "count": {
      "type": "integer",
      "default": 0,
      "minimum": 0
    },
    "ingredients": {
      "type": "array",
      "default": [],
      "items": {
        "type": "string"
      }
    }
  }
}
